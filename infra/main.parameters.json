{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "resourceGroupName": {
      "value": "rg-${AZURE_ENV_NAME}"
    },
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "aiServicesName": {
      "value": "${AZURE_AISERVICES_NAME}"
    },
    "deploySharedPrivateLinks": {
      "value": "${AZURE_DEPLOY_SHARED_PRIVATE_LINKS}"
    },
    "allowedIpAddresses": {
      "value": "${ALLOWED_IP_ADDRESSES}"
    },
    "myPrincipalId": {
      "value": "${AZURE_PRINCIPAL_ID}"
    },
    "tags": {
      "value": {
        "Owner": "AI Team",
        "Project": "GPTBot",
        "Environment": "Dev",
        "Toolkit": "Bicep"
      }
    }
  }
}
